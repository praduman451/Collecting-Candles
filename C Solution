#include <stdio.h>
int main()
{ int sum1=0,k=0,j,temp=0,sum2,sum[100],n,i,a,b[10000];
 scanf("%d",&n);
 while(n--)
 {
   scanf("%d",&a);
   for(i=0;i<a;i++)
   {
     scanf("%d",&b[i]);
   }
   for(i=0;i<a;i++)
   
   { sum2=sum2+b[i];
     sum[k]=sum2;
	 k++;
      }
      for(i=1;i<k;i++)
	  { sum1=sum1+sum[i];
	     }        
     printf("%d",sum1); 
 }
    return 0;
}
